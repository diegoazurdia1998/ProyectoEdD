
@{
    ViewData["Title"] = "guardarDatos";
}

<p class="text-center">
    <h3 class="text-center">
        @if (!ViewBag.lleno)
        {
            @ViewBag.PacienteIngresado
            <p>
                guardado correctamente.
            </p>
        }
        else
        {
            <p>
                El día asignado está lleno reingrese el paceinte con otra cita.
            </p>
        }


    </h3>
    <h4 class="text-center">
        <form asp-action="Index">
            <input type="submit" value="Regresar al inicio" class="btn btn-info" />
        </form>
    </h4>
</p>
